<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="SmokeSuite" parallel="false">
    <listeners>
        <listener class-name="Listners.TestNGListeners"/>
    </listeners>

    <test name="SanityTests">
        <groups>
            <run>
                <include name="sanity"/>
            </run>
        </groups>
        <classes>
            <class name="createBookingTests"/>
            <class name="updateBookingTests"/>
            <class name="e2e"/>
        </classes>
    </test>
</suite>