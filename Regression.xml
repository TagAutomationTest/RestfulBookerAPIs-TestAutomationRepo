<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="RestfulBookerSuite" parallel="false">
    <!-- Registering Listeners -->
    <listeners>
        <listener class-name="Listners.TestNGListeners"/>
    </listeners>
    <test name="RegressionTests">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="createBookingTests"/>
            <class name="updateBookingTests"/>
            <class name="e2e"/>
        </classes>
    </test>

</suite>
